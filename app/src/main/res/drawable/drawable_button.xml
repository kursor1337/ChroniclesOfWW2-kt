<?xml version="1.0" encoding="utf-8"?>

<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:right="2dp"
        android:left="2dp"
        android:bottom="4dp"
        android:top="4dp"
        >
        <shape>
            <solid android:color="#ddffffff"/>
            <corners android:radius="2dp"/>

            <stroke
                android:color="#55000000"
                android:width="2dp"/>

        </shape>
    </item>

</layer-list>



